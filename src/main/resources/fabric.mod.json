    "cloth-config": "*",
    "modonomicon": "*",
    "trinkets": "*",
    "matchbooks": "*",
    "exclusions_lib": "*"
  },
  "suggests": {
    "chalk": "*",
    "chalk-colorful-addon": "*",
    "paginatedadvancements": "*",
    "ants": "*",
    "bettercombat": "*",
    "continuity": "*"
  },
  "breaks": {
    "idwtialsimmoedm": "<0.3.0-"
  }
