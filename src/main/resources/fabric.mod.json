{
  "schemaVersion": 1,
  "id": "spectrum",
  "version": "${version}",
  "name": "Spectrum",
  "description": "A full-feature magic mod about harnessing the powers of color",
  "authors": [
    "DaFuqs - Big Boss (Furry)",
    "Azzyypaaras - Easygoing egoist (Art and Writing, Gayest)",
    "Noaaan - Mythic Coder (Hard Carries on MC Updates)"
  ],
  "contributors": [
    "Electro_593 (A lot of code)",
    "Maya (Gay)",
    "HoneyHive (Illustrations)",
    "800020h (Structures)",
    "MsRandom (Gayer, Shaders)",
    "Quarx_ (Sound design and Music)",
    "Notawallplan (Music, Writing)",
    "Athebyne (Gay Dragon)"
  ],
  "contact": {
    "homepage": "https://github.com/DaFuqs/",
    "sources": "https://github.com/DaFuqs/Spectrum",
    "issues": "https://github.com/DaFuqs/Spectrum/issues"
  },
  "license": "lGPL3",
  "icon": "assets/spectrum/icon.png",
  "environment": "*",
